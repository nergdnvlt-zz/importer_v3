<%- headers = [
      'referenceId',
      'first',
      'last',
      'email',
      'company',
      'phone',
      'language',
      'country',
      'addressLine1',
      'addressLine2',
      'city',
      'region',
      'postalCode',
      'live',
      'accountId',
      'productPath',
      'quantity',
      'currency',
      'listPrice',
      'interval',
      'intervalLength',
      'beginDate',
      'trialEndDate',
      'nextChargeDate',
      'endDate',
      'intervalCount',
      'couponCode',
      'discountType',
      'discountValue',
      'discountDuration',
      'taxExemptionData',
      'manualRenew'
    ] -%>
<%= CSV.generate_line headers -%>
<%- @company.subscriptions.each do |sub| -%>
<%= CSV.generate_line([
    sub.reference_id,
    sub.first,
    sub.last,
    sub.email,
    sub.buyer_company,
    sub.phone,
    sub.language,
    sub.country,
    sub.address_1,
    sub.address_2,
    sub.city,
    sub.region,
    sub.postal_code,
    sub.live,
    sub.account_id,
    sub.product_path,
    sub.quantity,
    sub.currency,
    sub.list_price,
    sub.interval,
    sub.interval_length,
    sub.begin_date,
    sub.trial_end_date,
    sub.next_charge_date,
    sub.end_date,
    sub.interval_count,
    sub.couponCode,
    sub.discountType,
    sub.discountValue,
    sub.discountDuration,
    sub.taxExemptionData,
    sub.manualRenew
    ]) -%>
<%- end -%>